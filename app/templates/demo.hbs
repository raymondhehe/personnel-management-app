{{#d3-xy-chart options=options}}
  {{d3-line xScale=options.plot.xScale yScale=options.plot.yScale xAccessor=xAccessor yAccessor=yAccessor data=data}}
  {{#if options.plot.showDots}}
    {{d3-circle xScale=options.plot.xScale yScale=options.plot.yScale transition=d3Transition data=data}}
  {{/if}}
{{/d3-xy-chart}}
