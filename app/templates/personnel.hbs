
<div class="container-fluid">
    <div class="row">
      <div class="col-sm-2">
          {{personnel-nav}}
      </div>
      <div class="col-sm-10">

        <div class="col-sm-12">
          <h2>Active Personnel</h2>
          <hr>
        <div class="bs-example">
            <table class="table">
                <thead>
                    <tr>
                        <th>id</th>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>Years</th>
                    </tr>
                </thead>
                <tbody>
                  {{#each model as |personnel|}}

                    <tr>
                        <td>{{personnel.id}}</td>
                        <td>{{personnel.firstName}}</td>
                        <td>{{personnel.lastName}}</td>
                        <td>{{personnel.yrs}}</td>
                    </tr>

                    {{/each}}
                </tbody>
            </table>
        </div>
        </div>

      </div>
    </div>
</div>
