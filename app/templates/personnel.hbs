<div class="container-fluid">
    <div class="row">
      <div class="col-sm-2">
          {{personnel-nav}}
      </div>
      <div class="col-sm-10">

        <div class="col-sm-12">
          <h4>Active Personnel</h4>
          <hr>

          <div class="row">
            <div class="col-sm-2">
            <h4>Test</h4>
          </div>
              <div class="col-sm-10">
                      <div id="custom-search-input">
                            <div class="input-group col-sm-4">
                                <input type="text" class="  search-query form-control" placeholder="Search" />
                                <span class="input-group-btn">
                                    <button class="btn btn-danger" type="button">
                                        <span class=" glyphicon glyphicon-search"></span>
                                    </button>
                                </span>
                            </div>
                        </div>


                    </div>
              </div>

        <div class="bs-example">
            <table class="table">
                <thead>
                    <tr>
                        <th>Edit</th>
                        <th>id</th>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>Years</th>
                    </tr>
                </thead>
                <tbody>
                  {{#each model as |personnel|}}

                    <tr>
                        <td>

                        <a href="#">
                        <span class="glyphicon glyphicon-plus"></span>
                        </a>

                        </td>
                        <td>{{personnel.id}}</td>
                        <td>{{personnel.firstName}}</td>
                        <td>{{personnel.lastName}}</td>
                        <td>{{personnel.yrs}}</td>
                    </tr>

                    {{/each}}
                </tbody>
            </table>
        </div>
        </div>

      </div>
    </div>
</div>
